<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@android:drawable/btn_star_big_on"
        android:label="@string/certificationsapp"
        android:roundIcon="@android:drawable/btn_star_big_on"
        android:supportsRtl="true"
        android:theme="@style/Theme.TuristicAppAmov"
        tools:targetApi="31">
        <activity
            android:name=".myactivities.ResultsActivity"
            android:exported="false"
            android:label="@string/title_activity_results"
            android:theme="@style/Theme.TuristicAppAmov" />
        <activity
            android:name=".myactivities.TesteActivity"
            android:exported="false"
            android:label="@string/title_activity_teste"
            android:theme="@style/Theme.TuristicAppAmov" />
        <activity
            android:name=".myactivities.MenuActivity"
            android:exported="false"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.TuristicAppAmov" />
        <activity
            android:name=".myactivities.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.TuristicAppAmov">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>